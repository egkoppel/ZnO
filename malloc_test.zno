uses 'stdio.zno'

func main() {
	let x_ptr = stdio::allocate();
	stdio::printI(x_ptr);
	stdio::printI(*x_ptr);
	stdio::set_ptr(x_ptr, 7);
	stdio::printI(*x_ptr);
	stdio::deallocate(x_ptr);
	let y = 65;
	let y_ptr = &y;
	stdio::printI(y_ptr);
	stdio::printI(*y_ptr);
	*y_ptr = 5;
	stdio::printI(y_ptr);
	stdio::printI(*y_ptr);
	return;
}