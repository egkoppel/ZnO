{
    "problemMatcher": [
        {
            "owner": "gtest-problems",
            "pattern": [
                {
                    "regexp": "(.*)(?=:\\d+):(\\d+):\\s*(Failure)\n(([^/])*)(?!.*\/)",
                    "file": 1,
                    "line": 2,
                    "message": 4
                }
            ]
        }
    ]
}