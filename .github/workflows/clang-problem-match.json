{
    "problemMatcher": [
        {
            "owner": "clang-problems",
            "pattern": [
                {
                    "regexp": "(.*)(?=:\\d+:\\d):(\\d+):(\\d+):\\s*(warning|error):\\s*(.[^\\[\n]*)(\\[.*(?=\\])\\])?",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5,
                    "code": 6
                }
            ]
        }
    ]
}