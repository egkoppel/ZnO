{
    "problemMatcher": [
        {
            "owner": "diff-problems",
            "pattern": [
                {
                    "regexp": "^(Warning): ([^']*'([\\w/.]*)'.*)",
                    "file": 3,
                    "message": 2,
					"severity": 1
                },
				{
                    "regexp": "^([^:]*):",
                    "file": 1
                },
				{
                    "regexp": "^ output:(\\d+): (.*)",
					"line": 1,
                    "message": 2,
					"loop": true
				}
            ]
        }
    ]
}