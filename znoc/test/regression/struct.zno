uses 'stdio.zno'

struct OtherThingy {
	d: _fundamentals::i32,
	e: _fundamentals::i32
}

struct Thingy {
	a: _fundamentals::i32,
	b: _fundamentals::i32,
	c: OtherThingy
}

func printAStructThing(s: Thingy) -> _fundamentals::i32 {
	stdio::printI(s.c.d);
	return s.b;
}

func main() {
	let s: Thingy;
	s.a = 32;
	s.b = 65;
	s.c.d = 27;
	stdio::printI(printAStructThing(s));
	return;
}